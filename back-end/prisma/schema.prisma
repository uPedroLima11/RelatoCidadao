generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}



model Postagem {
  id          Int      @id @default(autoincrement())
  titulo      String
  descricao   String
  localizacao String
  foto        String
  estadoId    Int
  cidadeId    Int
  email       String 
  createdAt   DateTime @default(now())

  @@map("postagens")
}

